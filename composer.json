{
    "name": "symplify/default-autowire",
    "description": "Adds autowire to services when needed.",
    "keywords": ["Symfony", "autowire"],
    "license": "MIT",
    "require": {
        "php": "^5.6|^7.0",
        "symfony/config": "~2.8|~3.0",
        "symfony/dependency-injection": "~2.8|~3.0",
        "symfony/http-kernel": "~2.8|~3.0"
    },
    "require-dev": {
        "nette/utils": "^2.3",
        "phpunit/phpunit": "^5.1",
        "symplify/coding-standard": "^0.1"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\DefaultAutowire\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\DefaultAutowire\\Tests\\": "tests"
        }
    }
}
